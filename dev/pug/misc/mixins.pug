mixin links(className, items, links, svgs)
    ul(class=`${className}`)
        - for (var i = 0; i < items.length; i++)
            li(class=`${className}__item`)
                a(class=`${className}__link` href=links[i] target="_blank" title=`${items[i]}`)
                    svg(class=`${className}__svg`)
                        use(xlink:href="images/sprites/svg/sprite.svg#" + `${svgs[i]}`)

mixin title(title, svg)
    .title-wrapper
        - for (var i = 0; i < title.length; i++)
            svg.title-wrapper__svg
                use(xlink:href="images/sprites/svg/sprite.svg#" + `${svg[i]}`)
            h1.title= `${title[i]}`